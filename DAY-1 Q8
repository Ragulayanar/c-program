Float Formation

#include <math.h>
#include <stdio.h>

int main() {
    float num;
    scanf("%f", &num);

    printf("%f\n", num);         // Default float precision (usually 6 digits)
    printf("%.4f\n", num);       // 4 decimal places
    printf("%.2f\n", num);       // 2 decimal places
   printf("%d\n", (int)(num + 0.5)); // Round and convert to int

    return (0);
}
