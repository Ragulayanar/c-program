Harshad number
#include <stdio.h>

int main() {
    int number, temp, digitSum = 0;

   
    scanf("%d", &number);

    temp = number;

    
    while (temp > 0) {
        digitSum += temp % 10;
        temp /= 10;
    }

    
    if (digitSum != 0 && number % digitSum == 0) {
        printf("Harshad Number\n");
    } else {
        printf("Not Harshad Number\n");
    }

    return 0;
}





