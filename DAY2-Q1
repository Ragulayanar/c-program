EVEN OR ODD
#include <stdio.h>
  

int main() {
    int num;

    // Read an integer from user
    scanf("%d", &num);

    // Check if the number is divisible by 2
    if (num % 2 == 0) {
        printf("The given input %d is Even\n", num);
    } else {
        printf("The given input %d is Odd\n", num);
    }

    return 0;
}
