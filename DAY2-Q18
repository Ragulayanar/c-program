#include <stdio.h>

int main() {
    int num;
    scanf("%d", &num);


    if (num >= 10 && num <= 99) {
        int digit1 = num / 10;
        int digit2 = num % 10;
        int sum = digit1 + digit2;


        printf("Sum of Digit %d is %03d\n", num, sum);
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
